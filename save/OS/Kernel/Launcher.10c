:launch_programme
ife z , 1
	JSR prog_test
ife z , 0
	JSR prog_unknown



:fin_launcher
set pc , pop

:prog_test
set push , z
set z , 0x2
JSR affiche_commande
JSR affiche_message
set z , pop
set pc , fin_launcher

:prog_unknown
set push , z
set z , 0x1
JSR affiche_commande
JSR affiche_message
set z , pop
set pc , fin_launcher
