; =======================
; FFAT16_deleteFile
; Arguments :
; -----------
; B : Pointeur vers le nom de fichier.
; Retourne :
; ----------
; EX : 0 si pas d'erreur, sinon code d'erreur
; =======================

:FFAT16_deleteFile
	set EX, 0
	jsr push_regs
	
	set B, Y
	jsr FFAT16_getFileByName
	ifn EX, 0
		set PC, FFAT16_deleteFile_break0
	
	:FFAT16_deleteFile_break0
	
	jsr pop_regs

	set PC, POP
